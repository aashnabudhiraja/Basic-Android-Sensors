<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Accel">

    <TextView
        android:id="@+id/accel_z_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="220dp"
        android:layout_marginTop="8dp"
        android:text="Z"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_y_textview"
        app:layout_constraintVertical_bias="0.797" />

    <TextView
        android:id="@+id/accel_y_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="8dp"
        android:text="Y"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_x_textview"
        app:layout_constraintVertical_bias="0.237" />

    <TextView
        android:id="@+id/accel_x_textview"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginTop="64dp"
        android:text="X"
        android:textSize="30sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/accel_y_label"
        android:layout_width="wrap_content"
        android:layout_height="20dp"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="156dp"
        android:text="y value :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/accel_y_textview"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_x_label"
        app:layout_constraintVertical_bias="0.014" />

    <TextView
        android:id="@+id/accel_z_label"
        android:layout_width="wrap_content"
        android:layout_height="18dp"
        android:layout_marginBottom="232dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="z value :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/accel_z_textview"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_y_label"
        app:layout_constraintVertical_bias="0.814" />

    <TextView
        android:id="@+id/accel_x_label"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="8dp"
        android:layout_marginEnd="8dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:text="x value :"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/accel_x_textview"
        app:layout_constraintHorizontal_bias="0.496"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.1" />

    <TextView
        android:id="@+id/accel_x_min_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="xMin"
        app:layout_constraintBottom_toTopOf="@+id/accel_y_label"
        app:layout_constraintEnd_toStartOf="@+id/accel_x_max_text"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_x_label" />

    <TextView
        android:id="@+id/accel_x_max_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="xMax"
        app:layout_constraintBottom_toTopOf="@+id/accel_y_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_x_textview" />

    <TextView
        android:id="@+id/accel_y_min_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="yMin"
        app:layout_constraintBottom_toTopOf="@+id/accel_z_label"
        app:layout_constraintEnd_toStartOf="@+id/accel_y_max_text"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_y_label" />

    <TextView
        android:id="@+id/accel_y_max_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="yMax"
        app:layout_constraintBottom_toTopOf="@+id/accel_z_textview"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_y_textview" />

    <TextView
        android:id="@+id/accel_z_min_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="zMin"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/accel_z_max_text"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_z_label" />

    <TextView
        android:id="@+id/accel_z_max_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="zMax"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/accel_z_textview"
        app:layout_constraintVertical_bias="0.476" />

</android.support.constraint.ConstraintLayout>